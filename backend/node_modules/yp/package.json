{
  "name": "yp",
  "version": "2.0.3",
  "description": "publish static resources",
  "scripts": {
    "construct": "cd sample && node --harmony_proxies --harmony_default_parameters  ../bin/cli.js",
    "test": "node ./node_modules/mocha/bin/mocha --harmony_proxies --harmony_default_parameters ",
    "cov": "node --harmony_proxies--harmony_rest_parameters ./node_modules/istanbul/lib/cli.js  cover ./node_modules/mocha/bin/_mocha --hook-run-in-context --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/renaesop/pss"
  },
  "keywords": [
    "static",
    "resources",
    "front-end"
  ],
  "dependencies": {
    "co": "^4.6.0",
    "harmony-reflect": "^1.4.2",
    "mkdirp": "^0.5.1",
    "yp-css": "latest"
  },
  "bin": {
    "pss": "bin/start.js"
  },
  "author": "yuji",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.0.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3"
  },
  "engines":{
    "node" : ">=5.0"
  }
}